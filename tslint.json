{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [true, { "singleQuote": true }],
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-empty": [true, "allow-empty-catch"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
